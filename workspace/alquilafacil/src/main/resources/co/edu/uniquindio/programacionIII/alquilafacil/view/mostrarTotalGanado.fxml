<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.programacionIII.alquilafacil.controllers.MotrarTotalGanadoController">
   <children>
      <Label fx:id="lblTitle" styleClass="titulo" text="Label" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Label fx:id="lblFechaIni" styleClass="texto" text="Label" />
            <DatePicker fx:id="dtpFechaIni" />
            <Label fx:id="lblFechaFin" styleClass="texto" text="Label" />
            <DatePicker fx:id="dtpFechaFin" />
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="20.0">
         <children>
            <Button fx:id="btnVolver" mnemonicParsing="false" onAction="#volverEvent" styleClass="boton" text="Button" />
            <Button fx:id="btnMostrarGanado" mnemonicParsing="false" onAction="#mostrarGanadoEvent" styleClass="boton" text="Button" />
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@../styles/buttonStyle.css" />
      <URL value="@../styles/datePickerStyle.css" />
      <URL value="@../styles/txtFieldStyle.css" />
      <URL value="@../styles/textTitleStyle.css" />
   </stylesheets>
</VBox>
